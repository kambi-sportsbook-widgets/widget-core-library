{
  "name": "widget-core-library",
  "version": "3.1.0",
  "description": "",
  "main": "dist/core.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prestart": "npm run clean",
    "start": "env NODE_ENV=development webpack --watch --config webpack.config.js --verbose --display-modules",
    "prebuild": "npm run clean",
    "build": "env NODE_ENV=production webpack --config webpack.config.js --verbose --display-modules",
    "jsdoc": "rm -rf ./docs && jsdoc ./src/ -r -c jsdoc.conf.json -u ./tutorials/ -d ./docs/ -R ./README.md -t ./node_modules/ink-docstrap/template",
    "preversion": "npm run jsdoc",
    "jsdoc-help": "jsdoc --help"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/kambi-sportsbook-widgets/widget-library.git"
  },
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/kambi-sportsbook-widgets/widget-core-library#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.2",
    "node-sass": "^3.9.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "compass-mixins": "^0.12.10",
    "es6-promise": "^3.2.1"
  }
}
