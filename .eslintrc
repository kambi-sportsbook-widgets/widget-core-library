{
  "env": {
    "browser":true,
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "angular":false,
    "$":false,
    "Ext":false,
    "CopyWebpackPlugin": true,
    "MobileDetect":true,
    "FastClick":true,
    "FB":true,
    "gulp": true,
    "addToHomeScreen":true,
    "rivets":true,
    "path": true,
    "Stapes":true,
    "Promise":true,
    "CoreLibrary":true,
    "fetch":true,
    "sightglass":true,
    "webpack": true,
    "webpack-load-plugins": true
  },
  "rules":{
    "array-callback-return": 0,
    "arrow-body-style": 0,
    "arrow-spacing": 0,
    "block-scoped-var": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "dot-notation": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "indent": [2, 3, {"SwitchCase":1}],
    "linebreak-style": 0,
    "max-depth":[2, 8],
    "max-len":[2, {"code": 160, "ignoreComments": true}],
    "max-params": [2,10],
    "no-cond-assign": 0,
    "no-console": 0,
    "no-debugger": 0,
    "no-else-return": 0,
    "no-eval": 0,
    "no-iterator": 0,
    "no-mixed-operators": 0,
    "no-multi-str": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-proto": 0,
    "no-prototype-builtins": 0,
    "no-redeclare": 0,
    "no-restricted-syntax": 0,
    "no-script-url": 0,
    "no-shadow": 0,
    "no-unneeded-ternary": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "prefer-arrow-callback": 0,
    "padded-blocks": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "react/require-extension": "off",
    "require-yield": 0,
    "semi": 0,
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "strict": 0,
    "valid-typeof": 0,
    "vars-on-top": 0,
    "wrap-iife": [2, "any"]
  }
}